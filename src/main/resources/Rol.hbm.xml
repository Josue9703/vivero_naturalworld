<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="sv.edu.udb.www.vivero_naturalworld.entities.Rol" table="rol" schema="vivero_naturalworld">
        <id name="idRol">
            <column name="id_rol" sql-type="varchar(50)" length="50"/>
